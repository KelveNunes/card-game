    <main id="login" class="container text-center">
        <div class="justify-content-center align-itens-center row" id="conteiner-login">
            <form action="/cad" method="POST" form id="login" class="col-4">
                <img class="my-5 mx-auto" src="/public/imgs/card-game-cards-hold-512.webp" alt="" width="72" height="69">
                <h1 class="h3 mb-3 fw-normal">
                        Informações de login
                </h1>
                <div class="form-floating">
                    <input type="email" class="my-2 form-control" id="floatingInput" placeholder="name@exemple.com">
                    <label for="floatingInput">Email</label>
                </div>
                <div class="form-floating">
                    <input type="password" placeholder="Senha" class="my-2 form-control" id="floatingPassword">
                    <label for="floatingPassword">Senha</label>
                </div>
                
                <button class="w-100 my-2 btn btn-dark btn-lg" name="btLogin" type="submit" id="btn-login">Login</button> 

                <nav aria-label="Page navigation example">
                    <ul class="pagination justify-content-center">
                        <li class="page-item disabled">
                        </li>
                        <a class="page-link" href="/recSenha">Esqueceu a Senha?</a>
                        </li>
                    </ul>
                </nav>
                
                <button class="w-100 btn btn-dark btn-lg" name="cadastro" type="submit" id="btn-criar-conta" ><a href="/cadastro">Criar nova conta</a></button>
                
               
            </form>
        </div>
    </main>